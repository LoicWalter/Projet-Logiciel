{
  "name": "database",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "dependencies": {
    "@prisma/client": "^5.14.0"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "ts-node ./prisma/seed.ts",
    "db:rm": "docker-compose rm mongo -s -f -v",
    "db:up": "docker-compose up mongo -d",
    "db:start": "npm run db:rm && npm run db:up && sleep 5 && npm run db:push && npm run db:generate && npm run db:seed"
  },
  "devDependencies": {
    "prisma": "^5.14.0"
  }
}
